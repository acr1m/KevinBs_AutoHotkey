style.*.32=fore:#CCCCCC,back:#21272a,font:Bitstream Vera Sans Mono,size:10


# Caret Foreground
######################################################################
caret.fore=#FFFFFF

# Caret, Current Line Background
######################################################################
#~ caret.line.back=#151515
caret.line.back=#232e3f

# Whitespace Characters
######################################################################
#~ whitespace.fore=#3f4a50
whitespace.fore=#353f44

# Indent Guide Colors
######################################################################
#~ Style 37 is used for displaying indentation guides. Only the fore and back are used.
#~ style.*.37=fore:#353f44
style.*.37=fore:#52626a


# Selection
######################################################################
#~ selection.back=#00005e
#~ selection.back=#151718
#~ selection.back=#737b7e
